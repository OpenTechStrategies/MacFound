                    {%- endif -%}
                    {%- if not country_data["shown"] -%}
                        {%- set _ = country_data.update({"shown": True }) %}
=== {{ country_name }} ===
{% endif -%}
* {{ toc_lines[proposal_id] }}
{% endif -%}
{% endfor %}
        {%- endfor %}
    {%- endfor %}
